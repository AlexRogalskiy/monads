{
  "name": "@hqoss/monads",
  "version": "0.5.0",
  "description": "Type safe Option, Result, and Either types; inspired by Rust",
  "main": "./build/lib/index.js",
  "typings": "./build/lib/index.d.ts",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --loglevel warn --write \"**/*.{ts,js,json,yaml}\"",
    "format:check": "prettier --loglevel warn --check \"**/*.{ts,js,json,yaml}\"",
    "test": "jest ./test/**/*.ts",
    "generate-docs": "rm -rf ./docs && typedoc --mode file --excludePrivate --excludeNotExported --plugin typedoc-plugin-markdown --out ./docs && rm ./docs/README.md",
    "preversion": "npm run format:check && npm run build && npm test",
    "prepublishOnly": "npm run format:check && npm run build && npm test",
    "postversion": "git push --follow-tags"
  },
  "keywords": [
    "monads",
    "option",
    "result",
    "either",
    "javascript",
    "js",
    "typescript",
    "node",
    "node.js"
  ],
  "author": "Slavo Vojacek",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "26.0.24",
    "jest": "27.0.6",
    "prettier": "2.3.2",
    "ts-jest": "27.0.4",
    "typedoc": "0.21.4",
    "typedoc-plugin-markdown": "3.10.4",
    "typescript": "4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sniptt/monads.git"
  }
}
